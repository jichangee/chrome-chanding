{
  "name": "禅定",
  "description": "专注工作与学习",
  "version": "0.0.1",
  "content_scripts": [
    {
      "js": ["content_scripts.js"],
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
      "default_title": "专注工作与学习",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
